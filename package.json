{
  "name": "nash-and-smashed-webapp",
  "version": "1.0.1",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js --hot",
    "build:static": "node scripts/generate-static-templates.js",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "build": "node scripts/generate-static-templates.js && webpack --config webpack/webpack.prod.js",
    "deploy": "NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "tree": "tree -I 'node_modules|.git|dist|_notes|python'",
    "tree:output": "tree -I 'node_modules|.git|dist|notes|python|shine-venv' -o tree-directory-structure.txt",
    "refresh": "sudo rm -rf node_modules package-lock.json && rm -rf dist && npm install",
    "errors": "npm run build > build_errors.txt 2>&1",
    "generate-webp": "node scripts/generate-webp.js",
    "build:clean": "rm -rf dist webpack/templates && npm run build"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
