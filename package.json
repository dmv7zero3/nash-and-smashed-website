    "@shadcn/ui": "^0.0.4",
    "@stripe/stripe-js": "^5.5.0",
    "@types/react-datepicker": "^7.0.0",
    "axios": "^1.6.7",
    "better-auth": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "file-saver": "^2.0.5",
    "formik": "^2.4.6",
    "framer-motion": "^12.0.6",
    "fs": "^0.0.1-security",
    "gsap": "^3.12.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.330.0",
    "papaparse": "^5.4.1",
